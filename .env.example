
# BSC Sniper Bot 2.0 Environment Variables
# Copy this file to .env and fill in your values

# Your wallet private key (64 hex characters, with or without 0x prefix)
# Example: PRIVATE_KEY=abcd1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcd
PRIVATE_KEY=your_64_character_hex_private_key_here

# Telegram Bot Configuration (optional but recommended)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHANNEL_ID=your_telegram_channel_id_here

# BSC RPC URLs (backup endpoints)
BSC_RPC_URL_1=https://bsc-dataseed1.binance.org/
BSC_RPC_URL_2=https://bsc-dataseed2.binance.org/
BSC_RPC_URL_3=https://bsc-dataseed3.binance.org/

# Security Settings
ENABLE_TESTNET=false
MAX_GAS_PRICE=20
